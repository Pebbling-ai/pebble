{
  "author": "Pebbling Team",
  "name": "News Reporter Agent",
  "description": "An AI agent that reports current news with storytelling flair",
  "version": "1.0.0",
  "recreate_keys": true,
  "skills": [
    {
      "name": "news-reporting",
      "description": "Ability to gather and report current news",
      "version": "1.0.0"
    },
    {
      "name": "storytelling",
      "description": "Ability to present information in engaging narrative format",
      "version": "1.0.0"
    }
  ],
  "capabilities": {
    "reasoning": true,
    "code_execution": false,
    "web_search": true,
    "file_operations": false,
    "multimodal": false
  },
  "agent_trust": "medium",
  "kind": "agent",
  "debug_mode": false,
  "debug_level": 1,
  "monitoring": true,
  "telemetry": true,
  "num_history_sessions": 10,
  "documentation_url": "https://docs.pebbling.ai/agents/news-reporter",
  "extra_metadata": {
    "domain": "journalism",
    "specialization": "current_events"
  },
  "deployment": {
    "url": "http://localhost:9001",
    "expose": true,
    "protocol_version": "1.0.0",
    "proxy_urls": ["https://news-reporter.pebbling.ai"],
    "cors_origins": ["*"]
  }
}
