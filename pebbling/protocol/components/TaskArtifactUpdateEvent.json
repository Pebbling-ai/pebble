{
  "$defs": {
    "Artifact": {
      "description": "Represents an artifact generated for a task.",
      "properties": {
        "artifact_id": {
          "description": "Unique identifier for the artifact",
          "format": "uuid",
          "title": "Artifact Id",
          "type": "string"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "parts": {
          "items": {
            "$ref": "#/$defs/Part"
          },
          "title": "Parts",
          "type": "array"
        }
      },
      "required": [
        "artifact_id",
        "parts"
      ],
      "title": "Artifact",
      "type": "object"
    },
    "DataPart": {
      "description": "Represents a structured data part in a message",
      "properties": {
        "kind": {
          "const": "data",
          "default": "data",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "content": {
          "title": "Content",
          "type": "string"
        },
        "data": {
          "additionalProperties": true,
          "title": "Data",
          "type": "object"
        }
      },
      "required": [
        "content",
        "data"
      ],
      "title": "DataPart",
      "type": "object"
    },
    "FilePart": {
      "description": "Represents a file part in a message",
      "properties": {
        "kind": {
          "const": "file",
          "default": "file",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "content": {
          "title": "Content",
          "type": "string"
        },
        "file": {
          "anyOf": [
            {
              "$ref": "#/$defs/FileWithBytes"
            },
            {
              "$ref": "#/$defs/FileWithUri"
            }
          ],
          "title": "File"
        }
      },
      "required": [
        "content",
        "file"
      ],
      "title": "FilePart",
      "type": "object"
    },
    "FileWithBytes": {
      "description": "File representation with binary content",
      "properties": {
        "bytes": {
          "title": "Bytes",
          "type": "string"
        },
        "mimeType": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Mimetype"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        }
      },
      "required": [
        "bytes"
      ],
      "title": "FileWithBytes",
      "type": "object"
    },
    "FileWithUri": {
      "description": "File representation with URI reference",
      "properties": {
        "bytes": {
          "title": "Bytes",
          "type": "string"
        },
        "mimeType": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Mimetype"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "uri": {
          "title": "Uri",
          "type": "string"
        }
      },
      "required": [
        "bytes",
        "uri"
      ],
      "title": "FileWithUri",
      "type": "object"
    },
    "Part": {
      "anyOf": [
        {
          "$ref": "#/$defs/TextPart"
        },
        {
          "$ref": "#/$defs/FilePart"
        },
        {
          "$ref": "#/$defs/DataPart"
        }
      ],
      "description": "Union type for all possible message parts",
      "title": "Part"
    },
    "TextPart": {
      "description": "Represents a text segment within parts.",
      "properties": {
        "kind": {
          "const": "text",
          "default": "text",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "content": {
          "title": "Content",
          "type": "string"
        }
      },
      "required": [
        "content"
      ],
      "title": "TextPart",
      "type": "object"
    }
  },
  "description": "Event for task artifact updates",
  "properties": {
    "append": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Append"
    },
    "artifact": {
      "$ref": "#/$defs/Artifact"
    },
    "contextId": {
      "format": "uuid",
      "title": "Contextid",
      "type": "string"
    },
    "kind": {
      "const": "artifact-update",
      "default": "artifact-update",
      "title": "Kind",
      "type": "string"
    },
    "lastChunk": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Lastchunk"
    },
    "metadata": {
      "anyOf": [
        {
          "additionalProperties": true,
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Metadata"
    },
    "taskId": {
      "format": "uuid",
      "title": "Taskid",
      "type": "string"
    }
  },
  "required": [
    "artifact",
    "contextId",
    "taskId"
  ],
  "title": "TaskArtifactUpdateEvent",
  "type": "object"
}