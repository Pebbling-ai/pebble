{
  "$defs": {
    "Artifact": {
      "description": "Represents an artifact generated for a task.",
      "properties": {
        "artifact_id": {
          "description": "Unique identifier for the artifact",
          "format": "uuid",
          "title": "Artifact Id",
          "type": "string"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "parts": {
          "items": {
            "$ref": "#/$defs/Part"
          },
          "title": "Parts",
          "type": "array"
        }
      },
      "required": [
        "artifact_id",
        "parts"
      ],
      "title": "Artifact",
      "type": "object"
    },
    "DataPart": {
      "description": "Represents a structured data part in a message",
      "properties": {
        "kind": {
          "const": "data",
          "default": "data",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "content": {
          "title": "Content",
          "type": "string"
        },
        "data": {
          "additionalProperties": true,
          "title": "Data",
          "type": "object"
        }
      },
      "required": [
        "content",
        "data"
      ],
      "title": "DataPart",
      "type": "object"
    },
    "FilePart": {
      "description": "Represents a file part in a message",
      "properties": {
        "kind": {
          "const": "file",
          "default": "file",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "content": {
          "title": "Content",
          "type": "string"
        },
        "file": {
          "anyOf": [
            {
              "$ref": "#/$defs/FileWithBytes"
            },
            {
              "$ref": "#/$defs/FileWithUri"
            }
          ],
          "title": "File"
        }
      },
      "required": [
        "content",
        "file"
      ],
      "title": "FilePart",
      "type": "object"
    },
    "FileWithBytes": {
      "description": "File representation with binary content",
      "properties": {
        "bytes": {
          "title": "Bytes",
          "type": "string"
        },
        "mimeType": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Mimetype"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        }
      },
      "required": [
        "bytes"
      ],
      "title": "FileWithBytes",
      "type": "object"
    },
    "FileWithUri": {
      "description": "File representation with URI reference",
      "properties": {
        "bytes": {
          "title": "Bytes",
          "type": "string"
        },
        "mimeType": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Mimetype"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "uri": {
          "title": "Uri",
          "type": "string"
        }
      },
      "required": [
        "bytes",
        "uri"
      ],
      "title": "FileWithUri",
      "type": "object"
    },
    "Message": {
      "description": "Message exchanged between agents or users",
      "properties": {
        "contextId": {
          "format": "uuid",
          "title": "Contextid",
          "type": "string"
        },
        "kind": {
          "const": "message",
          "default": "message",
          "title": "Kind",
          "type": "string"
        },
        "messageId": {
          "format": "uuid",
          "title": "Messageid",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "parts": {
          "items": {
            "$ref": "#/$defs/Part"
          },
          "title": "Parts",
          "type": "array"
        },
        "role": {
          "$ref": "#/$defs/Role"
        },
        "extra_data": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Extra Data"
        }
      },
      "required": [
        "contextId",
        "messageId",
        "parts",
        "role"
      ],
      "title": "Message",
      "type": "object"
    },
    "Part": {
      "anyOf": [
        {
          "$ref": "#/$defs/TextPart"
        },
        {
          "$ref": "#/$defs/FilePart"
        },
        {
          "$ref": "#/$defs/DataPart"
        }
      ],
      "description": "Union type for all possible message parts",
      "title": "Part"
    },
    "Role": {
      "description": "Message sender's role",
      "enum": [
        "agent",
        "user"
      ],
      "title": "Role",
      "type": "string"
    },
    "Task": {
      "description": "Top-level task representation",
      "properties": {
        "artifacts": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/Artifact"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Artifacts"
        },
        "contextId": {
          "format": "uuid",
          "title": "Contextid",
          "type": "string"
        },
        "history": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/Message"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "History"
        },
        "id": {
          "title": "Id",
          "type": "string"
        },
        "kind": {
          "const": "task",
          "default": "task",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "status": {
          "$ref": "#/$defs/TaskStatus"
        }
      },
      "required": [
        "contextId",
        "id",
        "status"
      ],
      "title": "Task",
      "type": "object"
    },
    "TaskState": {
      "description": "Represents the possible states of a Task.",
      "enum": [
        "submitted",
        "working",
        "input-required",
        "completed",
        "canceled",
        "failed",
        "rejected",
        "auth-required",
        "unknown",
        "trust-verification-required"
      ],
      "title": "TaskState",
      "type": "string"
    },
    "TaskStatus": {
      "description": "Status information for a task",
      "properties": {
        "message": {
          "anyOf": [
            {
              "$ref": "#/$defs/Message"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "state": {
          "$ref": "#/$defs/TaskState"
        },
        "timestamp": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ISO-8601 timestamp of the task status update",
          "examples": [
            "2023-10-27T10:00:00Z"
          ],
          "title": "Timestamp"
        }
      },
      "required": [
        "state"
      ],
      "title": "TaskStatus",
      "type": "object"
    },
    "TextPart": {
      "description": "Represents a text segment within parts.",
      "properties": {
        "kind": {
          "const": "text",
          "default": "text",
          "title": "Kind",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Metadata"
        },
        "content": {
          "title": "Content",
          "type": "string"
        }
      },
      "required": [
        "content"
      ],
      "title": "TextPart",
      "type": "object"
    }
  },
  "description": "Success response for canceling a task",
  "properties": {
    "id": {
      "format": "uuid",
      "title": "Id",
      "type": "string"
    },
    "jsonrpc": {
      "const": "2.0",
      "default": "2.0",
      "title": "Jsonrpc",
      "type": "string"
    },
    "result": {
      "$ref": "#/$defs/Task"
    }
  },
  "required": [
    "id",
    "result"
  ],
  "title": "CancelTaskSuccessResponse",
  "type": "object"
}